 access_id_CA      X509     '/C=HU/O=NIIF/OU=Certificate Authorities/CN=NIIF Root CA 2'
 pos_rights        globus   CA:sign
 cond_subjects     globus   '"/C=HU/O=NIIF/OU=Certificate Authorities/CN=NIIF Root CA 2" "/C=HU/O=NIIF CA/OU=GRID/*" "/C=HU/O=NIIF CA/OU=NIIF/OU=GRID/*"'
